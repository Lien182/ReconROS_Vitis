{
    "name": "Xilinx Hash Group Aggregate Benchmark",
    "description": "Xilinx Hash Group Aggregate Benchmark",
    "flow": "vitis",
    "gui": true,
    "platform_type": "pcie",
    "platform_whitelist": [
        "u280",
        "u50"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u280": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_HBM"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u280.ini"
                    ]
                }
            }
        },
        "u50": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_HBM"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.ini"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "generic launch for sw_emu flows",
            "target": "sw_emu",
            "cmd_args": " -xclbin BUILD/hash_aggr_kernel.xclbin -rep 1 -key_column 1 -pld_column 1 -scale 100"
        },
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/hash_aggr_kernel.xclbin -rep 1 -key_column 1 -pld_column 1 -scale 10000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/hash_aggr_kernel.xclbin"
        }
    ],
    "host": {
        "host_exe": "test_aggr.exe",
        "compiler": {
            "sources": [
                "host/test_aggr.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/ext/xcl2",
                "kernel",
                "LIB_DIR/L1/include/hw"
            ],
            "options": "-O3 ",
            "symbols": [
                "USE_HBM"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "kernel",
                "host",
                "LIB_DIR/../utils/L1/include"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/hash_aggr_kernel.cpp",
                    "frequency": 250,
                    "name": "hash_aggr_kernel"
                }
            ],
            "frequency": 250,
            "name": "hash_aggr_kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
