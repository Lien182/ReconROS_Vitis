{
    "name": "Xilinx Hash Join V3 Benchmark",
    "description": "Xilinx Hash Join V3 Benchmark",
    "flow": "vitis",
    "gui": true,
    "platform_type": "pcie",
    "platform_whitelist": [
        "u280",
        "u50"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "platform_properties": {
        "u280": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u280.ini"
                    ]
                }
            }
        },
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u50.ini"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "generic launch for sw_emu flows",
            "target": "sw_emu",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin -scale 10"
        },
        {
            "name": "generic launch for hw_emu flows",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin -scale 5000"
        },
        {
            "name": "generic launch for hw flows",
            "target": "hw",
            "cmd_args": " -xclbin BUILD/hash_join_v3.xclbin"
        }
    ],
    "host": {
        "host_exe": "test_join.exe",
        "compiler": {
            "sources": [
                "host/test_join.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "host",
                "LIB_DIR/ext/xcl2",
                "kernel"
            ],
            "options": "-O3 ",
            "symbols": []
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "kernel",
                "host"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "kernel/join_kernel.cpp",
                    "frequency": 300,
                    "clflags": " -D KERNEL_NAME=join_kernel",
                    "name": "join_kernel"
                }
            ],
            "frequency": 300,
            "name": "hash_join_v3"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 470,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
